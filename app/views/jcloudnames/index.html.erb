<h1>Listing jcloudnames</h1>

<table>
  <tr>
    <th>Provider</th>
    <th>Geo</th>
    <th>Providername</th>
    <th>Locationname</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jcloudnames.each do |jcloudname| %>
  <tr>
    <td><%= jcloudname.provider.name %></td>
    <td><%= jcloudname.geo.show %></td>
    <td><%= jcloudname.providername %></td>
    <td><%= jcloudname.locationname %></td>
    <td><%= link_to 'Show', jcloudname %></td>
    <td><%= link_to 'Edit', edit_jcloudname_path(jcloudname) %></td>
    <td><%= link_to 'Destroy', jcloudname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Jcloudname', new_jcloudname_path %>
