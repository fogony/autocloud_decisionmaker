<h1>Listing rates</h1>

<table>
  <tr>
    <th>Resourceset</th>
    <th>Servicetype</th>
    <th>Operatingsystem</th>
    <th>Geo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rates.each do |rate| %>
  <tr>
    <td><%= rate.resourceset.show %></td>
    <td><%= rate.servicetype.show %></td>
    <td><%= rate.operatingsystem.name %></td>
    <td><%= rate.geo.show %></td>
    <td><%= link_to 'Show', rate %></td>
    <td><%= link_to 'Edit', edit_rate_path(rate) %></td>
    <td><%= link_to 'Destroy', rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rateset', new_rate_path %>
